{
    "name": "armincms/location",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "armincms/targomaan": "~0.1",
        "armincms/nova-targomaan": "~0.1",
        "gobrightspot/nova-detached-actions": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Location\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\Location\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
